{
  "name": "solparse-exp-jb",
  "version": "3.0.3",
  "description": "PEG.js Solidity parser for Javascript, experimental work (jb) to support latest solidity features and work with vscode solidity, this should NOT be merged with the main repo as it includes support for incomplete statements",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "mkdir -p ./build && peggy --cache -o ./build/parser.js ./solidity.pegjs && peggy -o ./build/imports_parser.js ./imports.pegjs",
    "test": "mocha --timeout 5000 --reporter spec",
    "lint": "eslint index.js test/ cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanfranblanco/solparse.git"
  },
  "bin": {
    "solidity-parser": "./cli.js"
  },
